# Test new stdlib functions

# Test math functions
def test_math():
    print("Testing math functions:")
    print("math_pow(2, 3) =", math_pow(2.0, 3.0))
    print("math_sin(0) =", math_sin(0.0))
    print("math_cos(0) =", math_cos(0.0))
    print("math_abs(-5) =", math_abs(-5.0))
    print("math_floor(3.7) =", math_floor(3.7))
    print("math_ceil(3.2) =", math_ceil(3.2))
    print("math_round(3.5) =", math_round(3.5))
    print("math_log(2.718) =", math_log(2.718))
    print("math_exp(1) =", math_exp(1.0))
    print("")

# Test string functions  
def test_strings():
    print("Testing string functions:")
